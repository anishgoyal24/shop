{
  "name": "users-server",
  "version": "1.0.0",
  "main": "server.ts",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev server.ts",
    "build": "tsc",
    "build:dev": "cd ../client/ && ng build && gzipper --verbose ./dist -i js,css && cd dist/ && rm *.js *.css && cd ../../server/ && ts-node-dev server.ts",
    "build:prod": "cd ../client/ && ng build --prod && gzipper --verbose ./dist -i js,css && cd dist/ && rm *.js *.css && cd ../../server/ && NODE_ENV=production ts-node server.ts",
    "stage": "NODE_ENV=production ts-node server.ts",
    "prod": "NODE_ENV=production node server.js"
  },
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/mongoose": "^5.5.41",
    "@types/morgan": "^1.7.37",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-redis": "^1.0.25",
    "axios": "^0.19.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "farmhash": "^3.0.0",
    "moment": "^2.24.0",
    "mongoose": "^5.8.7",
    "morgan": "^1.9.1",
    "otp-generator": "^1.1.0",
    "otp-without-db": "^1.0.4",
    "socket.io": "^2.3.0",
    "socket.io-redis": "^5.2.0",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "@types/compression": "^1.0.1",
    "gzipper": "^3.4.2"
  }
}
